@model UNO.AppModel.EmployeeCardConfig
@{
    ViewBag.Title = "Delete";
}

@section scripts
{

    <script>
        $(document).ready(function () {
            $("#btnReset").click(function (e) {
                formmodified = 0;
                location.reload(true);
            });








        });
    </script>

}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)


    <div class="container-fluid">
        <!-- Spacer Starts -->
        <div class="spacer clearfix">
            <div class="col-md-7 col-centered">
                <div class="blog blog-default ">
                    <div class="blog-header ">
                        <h4 class="blog-title pull-left mt5">Delete </h4>
                        <span class="pull-right"><a class="btn btn-default" href='@Url.Action("index")'><i class="fa fa-arrow-left"></i> Back To List</a></span>
                        <span class="clearfix"></span>
                    </div>

                    <div class="blog-body form p0">

                        <div class="form-horizontal form-bordered form-label-stripped">

                            <div class="form-group">
                                @Html.LabelFor(m => m.CC_EMP_ID)
                                @Html.HiddenFor(m => m.CC_EMP_ID)
                            </div>


                            <div class="form-group">
                                @Html.LabelFor(m => m.ACTIVATION_DATE)
                                @Html.TextBoxFor(m => m.ACTIVATION_DATE, new { @class = "form-control", disabled = "disabled" })
                            </div>


                            <div class="form-group">
                                @Html.LabelFor(m => m.EXPIRY_DATE)
                                @Html.TextBoxFor(m => m.EXPIRY_DATE, new { @class = "form-control", disabled = "disabled" })
                            </div>


                            <div class="form-group">
                                @Html.LabelFor(m => m.USE_COUNT)
                                @Html.TextBoxFor(m => m.USE_COUNT, new { @class = "form-control", disabled = "disabled" })
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(m => m.STATUS)
                                @Html.DropDownListFor(modelItem => modelItem.STATUS, new[] { new SelectListItem { Text = "True", Value = "True" }, new SelectListItem { Text = "False", Value = "False" } })

                            </div>

                            <div class="form-actions">
                                <div class="row  mt5">
                                    <div class="col-md-12 text-center">
                                        <button type="submit" class="btn btn-sm btn-success" id="btnDelete"><i class="fa fa-save"></i>&nbsp;&nbsp;Delete</button>
                                        
                                    </div>
                                </div>
                            </div>






                        </div>
                    </div>



                </div>
            </div>
        </div>
    </div>



}
